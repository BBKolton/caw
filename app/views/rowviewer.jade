extends skeleton

block content
	.container

		- for (val in row)
			- if (val != 'createdAt' && val != 'deletedAt' && val != 'updatedAt')
				.form-group
					label(for='#{val}')= val[0].toUpperCase() + val.slice(1)
					- if (val == 'description')
						textarea.form-control(id='#{val}' name='#{val}' value='#{row[val]}')
					- else
						input.form-control(id='#{val}' name='#{val}' value='#{row[val]}')

		